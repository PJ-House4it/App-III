<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppIII.Maui.View.LoginPage">
    <ContentPage.Content>
        <VerticalStackLayout>
            <Entry WidthRequest="250" Placeholder="Brugernavn" Text="{Binding Username}" />
            <Entry WidthRequest="250" IsPassword="True" Placeholder="Password" Text="{Binding Password}" />
            <Button WidthRequest="250" Text="Login" Command="{Binding LoginCommand}" />
        </VerticalStackLayout>
    </ContentPage.Content>
</ContentPage>